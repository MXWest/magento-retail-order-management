			"url": "/home/stack/git/EcomDev_PHPUnit.git"
		},
		{
			"type": "vcs",
			"url": "/home/stack/git/magento-retail-order-management.git"
		},
		{
			"type": "vcs",
			"url": "/home/stack/git/magento-file-transfer.git"
		},
		{
			"type": "vcs",
			"url": "/home/stack/git/magento-log.git"
		},
		{
			"type": "vcs",
			"url": "/home/stack/git/php-dom.git"
		},
		{
			"type": "git",
			"url": "/home/stack/git/RetailOrderManagement-SDK.git"
		},
		{
			"type": "composer",
			"url": "http://packages.firegento.com"
		}
	],
	"extra": {
		"magento-deploystrategy": "copy",
		"magento-root-dir": "build/magento"
	}
