{
	"name": "ebayenterprise/magento-with-retail-order-management",
	"description": "eBay Enterprise Retail Order Management Magento Extension",
	"license": "OSL-3.0",
	"type": "project",
	"require": {
		"magento-hackathon/magento-composer-installer": "~2",
		"ebayenterprise/magento-retail-order-management": "~1.4.0"
	},
	"require-dev": {
		"ecomdev/ecomdev_phpunit": "dev-master as 0.3.x-dev",
		"firegento/psr0autoloader": "*",
