															<xs:documentation>Backordered</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
													<xs:enumeration value="T">
														<xs:annotation>
															<xs:documentation>Multiple Tracking for same line when split shipment happens wihout knowledge of the actual quantity in each box.</xs:documentation>
														</xs:annotation>
													</xs:enumeration>
												</xs:restriction>
											</xs:simpleType>
