                <observers>
                    <ebayenterprise_order_create_before_attach_fix_virtual_gift_cards>
                        <class>ebayenterprise_order/observer</class>
                        <method>handleEbayEnterpriseOrderCreateBeforeAttach</method>
                    </ebayenterprise_order_create_before_attach_fix_virtual_gift_cards>
                </observers>
