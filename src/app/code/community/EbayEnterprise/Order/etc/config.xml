    <frontend>
        <routers>
            <sales>
                <args>
                    <modules>
                        <ebayenterprise_order before="Mage_Sales">EbayEnterprise_Order</ebayenterprise_order>
                    </modules>
                </args>
            </sales>
        </routers>
        <layout>
            <updates>
                <ebayenterprise_order module="EbayEnterprise_Order">
                    <file>ebayenterprise_order.xml</file>
                </ebayenterprise_order>
            </updates>
        </layout>
        <translate>
            <modules>
                <EbayEnterprise_Order>
                    <files>
                        <default>EbayEnterprise_Order.csv</default>
                    </files>
                </EbayEnterprise_Order>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <EbayEnterprise_Order>
                    <files>
                        <default>EbayEnterprise_Order.csv</default>
                    </files>
                </EbayEnterprise_Order>
