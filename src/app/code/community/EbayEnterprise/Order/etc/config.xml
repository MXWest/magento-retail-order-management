                    <ebayenterprise_copy_discounts_observer>
                        <class>ebayenterprise_order/observer</class>
                        <method>handleSalesConvertQuoteAddressToOrderAddress</method>
                    </ebayenterprise_copy_discounts_observer>
                </observers>
            </sales_convert_quote_address_to_order_address>
