                    <!-- Users will still need to define a schedule. Please see the documentation. -->
                    <model>ebayenterprise_order/observer::handleEbayEnterpriseOrderCreateRetryJob</model>
                </run>
            </ebayenterprise_order_create_retry>
        </jobs>
    </crontab>
