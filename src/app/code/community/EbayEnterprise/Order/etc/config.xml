                    <order_print_shipment>EbayEnterprise_Order_Overrides_Block_Order_Print_Shipment</order_print_shipment>
                </rewrite>
            </sales>
            <enterprise_rma>
                <rewrite>
                    <order_info>EbayEnterprise_Order_Overrides_Block_Order_Rma_Info</order_info>
                </rewrite>
            </enterprise_rma>
            <checkout>
                <rewrite>
                    <onepage_success>EbayEnterprise_Order_Overrides_Block_Checkout_Onepage_Success</onepage_success>
                    <multishipping_success>EbayEnterprise_Order_Overrides_Block_Checkout_Multishipping_Success</multishipping_success>
                </rewrite>
            </checkout>
        </blocks>
        <models>
            <ebayenterprise_order>
                <class>EbayEnterprise_Order_Model</class>
                <resourceModel>ebayenterprise_order_resource</resourceModel>
            </ebayenterprise_order>
            <ebayenterprise_order_resource>
                <class>EbayEnterprise_Order_Model_Resource</class>
            </ebayenterprise_order_resource>
            <enterprise_rma>
                <rewrite>
                    <rma>EbayEnterprise_Order_Overrides_Model_Enterprise_Rma</rma>
                </rewrite>
            </enterprise_rma>
        </models>
        <resources>
            <ebayenterprise_order_setup>
                <setup>
                    <module>EbayEnterprise_Order</module>
                    <class>EbayEnterprise_Order_Model_Resource_Setup</class>
                </setup>
            </ebayenterprise_order_setup>
        </resources>
        <helpers>
            <ebayenterprise_order>
                <class>EbayEnterprise_Order_Helper</class>
            </ebayenterprise_order>
            <sales>
                <rewrite>
                    <data>EbayEnterprise_Order_Overrides_Helper_Sales</data>
                </rewrite>
            </sales>
        </helpers>
        <events>
            <ebayenterprise_amqp_message_order_credit_issued>
                <observers>
                    <order_credit_issued_observer>
                        <class>ebayenterprise_order/observer</class>
                        <method>handleEbayEnterpriseAmqpMessageOrderCreditIssued</method>
                    </order_credit_issued_observer>
                </observers>
            </ebayenterprise_amqp_message_order_credit_issued>
            <ebayenterprise_amqp_message_order_rejected>
                <observers>
                    <order_update_event_order_status_rejected>
                        <class>ebayenterprise_order/observer</class>
                        <method>handleEbayEnterpriseAmqpMessageOrderRejected</method>
                    </order_update_event_order_status_rejected>
                </observers>
            </ebayenterprise_amqp_message_order_rejected>
